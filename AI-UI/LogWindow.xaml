<Window x:Class="AI_UI.LogWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AI_UI"
        mc:Ignorable="d"
        Title="LogWindow" Height="800" Width="800" IsEnabled="False"
        Background="Black">
    <Grid>
        <ScrollBar HorizontalAlignment="Right"/>
        <TextBlock Name="LogBlock" Margin="0,0,17,0" Foreground="White" TextWrapping="Wrap"/>
    </Grid>
</Window>
