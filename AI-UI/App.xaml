<Application x:Class="AI_UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="StartupWindow.xaml">
    <Application.Resources>
        <Style TargetType="{x:Type Button}">
            <Setter Property="Background" Value="#FFA500" />
            <Setter Property="Foreground" Value="#013343" />
        </Style>
        <BitmapImage x:Key="AI-UI_logo" UriSource="images\AI-UI_logo.png"></BitmapImage>
        <BitmapImage x:Key="dice" UriSource="images\dice.png"></BitmapImage>
        <BitmapImage x:Key="Zahnrad" UriSource="images\Zahnrad.png"></BitmapImage>
    </Application.Resources>
</Application>

